{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listen for XDebug",
      "type": "php",
      "request": "launch",
      "port": 9001,
      "log": true,
      "xdebugSettings": {
        "max_children": 999,
        "max_depth": 10
      },
      "pathMappings": {
        "/app/": "${workspaceFolder}/",
      }
    }
  ]
}
